alias ..='cd ..'
alias c='clear'
alias ca='cargo'
alias cat='bat'
alias dt='cd ~/Desktop'
alias ea='v ~/dotfiles/zsh/aliases'
alias ee='v ~/dotfiles/zsh/env'
alias ef='v ~/dotfiles/zsh/functions'
alias ep='v ~/dotfiles/zsh/prompt'
alias l='exa --long --header --git --all --sort name'
alias la='exa -a --long --header --sort name'
alias lesss="less -FXRS"
alias ls='exa'
alias mkdir='mkdir -p'
alias o='open . &'
alias reload='source ~/.zshrc'
alias rustdocs='RUSTDOCFLAGS="--cfg docsrs" cargo +nightly doc --no-deps --open'
alias s='cd $(shellmark)'
alias stream='stream-countdown 10 -g davidpdrsn -t davidpdrsn'

alias vi='nvim'
alias vim='nvim'
alias vimconflicts='nvim $(rg -l -. "[<>]{5}")'
alias vv='nvim $(rg --files | fzf)'
alias xtask='cargo xtask'

